{
  "name": "leaguedex-app",
  "version": "0.0.52",
  "description": "Leaguedex is a tool for League of Legends players to track their games and analyze their performance.",
  "main": "./out/main/index.js",
  "author": "Donny Roufs",
  "homepage": "https://leaguedex.app",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "test": "vitest",
    "test:arch": "depcruise --config .dependency-cruiser.js src/main",
    "postinstall": "electron-builder install-app-deps",
    "publish-app": "electron-builder -p always",
    "build:unpack": "yarn build && electron-vite build && electron-builder --dir",
    "build:win": "yarn build && electron-vite build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-vite build && electron-builder --linux",
    "lefthook": "lefthook"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.536.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.7.1",
    "say": "^0.16.0",
    "sound-play": "^1.1.0",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@amiceli/vitest-cucumber": "^5.1.2",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sound-play": "^1.1.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "3.2.4",
    "dependency-cruiser": "^17.0.1",
    "dotenv-cli": "^10.0.0",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "lefthook": "^1.12.3",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  }
}